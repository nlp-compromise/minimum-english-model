{
  "name": "minimum-model",
  "description": "experimental compression of word conjugation models",
  "version": "0.0.0",
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "main": "./builds/minimum-model.mjs",
  "unpkg": "./builds/minimum-model.min.js",
  "module": "./builds/minimum-model.mjs",
  "types": "types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./builds/minimum-model.mjs",
      "require": "./builds/minimum-model.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/minimum-model.git"
  },
  "scripts": {
    "test": "tape \"./tests/**/*.test.js\" | tap-dancer  --color always",
    "testb": "TESTENV=prod tape \"./tests/**/*.test.js\" | tap-dancer  --color always",
    "watch": "amble ./scratch.js",
    "build": "rollup -c --silent"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "files": [
    "builds/"
  ],
  "eslintIgnore": [
    "builds/*.js"
  ],
  "devDependencies": {
    "amble": "1.3.0",
    "tap-dancer": "0.3.4",
    "tape": "5.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "xml-stream": "^0.4.5"
  }
}